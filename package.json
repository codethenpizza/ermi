  "scripts": {
    "start": "npm run set-max-space && node dist/index.js",
    "build": "tsc",
    "dev": "nodemon",
    "set-dev-env": "export NODE_ENV=development",
    "cmd": "ts-node -r tsconfig-paths/register src/scripts/commands.ts",
    "cmd-dev": "NODE_ENV=development npm run cmd",
    "migrate": "npm run cmd migrate",
    "reset-all-data": "yarn cmd sync-models 1 && yarn cmd migrate sync && yarn migrate && yarn cmd es reset-indexes",
    "runner": "npm run set-max-space && ts-node -r tsconfig-paths/register src/modules/suppliers/index.ts",
    "reindex": "npm run set-max-space && npm run cmd es update-index",
    "set-max-space": "export NODE_OPTIONS=--max_old_space_size=4096",
    "test": "jest"
  },
