    "express-fileupload": "^1.2.0",
    "express-jwt": "^6.0.0",
    "ftp": "^0.3.10",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "module-alias": "^2.2.2",
    "mysql2": "^2.1.0",
